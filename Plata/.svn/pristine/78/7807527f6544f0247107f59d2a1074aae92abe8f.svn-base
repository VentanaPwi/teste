 PROCEDURE ToolbarOnInit
 LPARAMETERS toToolbar
 LOCAL button
 IF toToolbar.AddObject("mybutton", "commandbutton")
  BINDEVENT(toToolbar.mybutton, "click" , this, "ButtonClicked")  
  toToolbar.mybutton.tabindex = 1
  toToolbar.mybutton.Visible = .t.
