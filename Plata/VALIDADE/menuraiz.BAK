* MENURAIZ.PRG - RAIZ DO VALIDADE
**************
LPARAMETERS tnP1, tcP2, tcP3
	IF TYPE('tnP1')<>'N'
		tnP1 = -1
	ENDIF
	IF TYPE('tcP2')<>'C'
		tcP2 = ''
	ENDIF	
	IF tnP1<0
		RETURN
	ENDIF
	SET TALK OFF
	SET ESCAPE OFF
	SET EXCL OFF
	SET DELE ON
	SET DATE BRITISH
	SET HOURS TO 24
	DO FORM VOLPE_VALIDADE WITH tnP1, tcP2
RETURN

FUNC CHAMAVALIDA
DO FORM VOLPE_VALIDADE WITH 0,'SQL'
RETURN(.T.)
